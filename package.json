{
  "name": "personal-server-egg",
  "version": "1.0.0",
  "description": "个人管家后台eggjs版本",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --env=prod --title=egg-server-person-server-egg",
    "stop": "egg-scripts stop --title=egg-server-person-server-egg",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "egg": "^2.26.0",
    "egg-mysql": "^3.0.0",
    "egg-passport": "^2.1.1",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.13.0",
    "egg-validate": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "loadsh": "0.0.4",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "@types/supertest": "^2.0.8",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.14.1",
    "egg-ci": "^1.13.1",
    "egg-mock": "^4.0.0",
    "tslib": "^1.11.1",
    "tslint": "^6.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "apy",
  "license": "MIT"
}
